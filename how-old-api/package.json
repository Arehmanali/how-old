{
  "name": "how-old-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "pm2": "pm2",
    "gulp": "gulp",
    "start": "nodemon --inspect ./bin/www",
    "start:staging": "pm2 start",
    "dev": "concurrently \"pm2 start\" \"gulp\"",
    "monitor": "pm2 monit",
    "reload-env": "pm2 reload --update-env ecosystem.config.js",
    "stop": "pm2 stop ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "knex": "knex --knexfile ./db/knexfile.js --debug"
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "browser-sync": "^2.27.11",
    "concurrently": "^7.6.0",
    "connect-redis": "^6.1.3",
    "debug": "~4.3.4",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "ejs": "~3.1.8",
    "express": "~4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.17.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-csscomb": "^3.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-html-prettify": "0.0.1",
    "gulp-imagemin": "^8.0.0",
    "gulp-npm-dist": "^1.0.3",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-useref-plus": "0.0.8",
    "gulp-util": "^3.0.8",
    "gulp-wait": "0.0.2",
    "http-errors": "~2.0.0",
    "joi": "^17.7.0",
    "knex": "^2.3.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0",
    "pm2": "^5.2.2",
    "postcss-flexbugs-fixes": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "packages/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
